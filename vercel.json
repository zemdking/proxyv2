{
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "dest": "https://example.com/$1",
      "headers": {
        "x-proxy-by": "vercel"
      }
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}
