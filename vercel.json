{
  "version": 2,
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "https://example.com/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "x-proxy-by",
          "value": "vercel"
        }
      ]
    }
  ]
}
